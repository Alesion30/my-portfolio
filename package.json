{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": "please_use_yarn_instead"
  },
  "scripts": {
    "dev": "run-p next:dev typed-scss-modules",
    "next:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "ssg": "next build && next export",
    "ssg-start": "npx http-server ./out",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typed-scss-modules": "typed-scss-modules src -w",
    "clean": "rm -rf node_modules yarn.lock .next"
  },
  "dependencies": {
    "@apollo/client": "3.7.2",
    "clsx": "1.2.1",
    "dayjs": "1.11.7",
    "framer-motion": "7.6.19",
    "graphql": "16.6.0",
    "modern-css-reset": "1.4.0",
    "next": "12.3.4",
    "next-pwa": "5.6.0",
    "next-seo": "5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.4.1",
    "react-markdown": "8.0.4",
    "react-use": "17.4.0",
    "three": "0.147.0",
    "vanta": "0.5.24"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.0.6",
    "@types/node": "18.11.13",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/three": "0.146.0",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "prettier-plugin-sort-json": "0.0.3",
    "sass": "1.56.2",
    "typed-scss-modules": "7.0.1",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@3.3.0"
}
